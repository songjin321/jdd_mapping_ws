<robot name="jd_robot">
  <material name="orange">
    <color rgba="1.0 0.5 0.2 1" />
  </material>
  <material name="gray">
    <color rgba="0.2 0.2 0.2 1" />
  </material>

  <link name="compensate_imu_link"/>

  <link name="rslidar"/>

  <link name="base_link" />

  <link name="output_link" />

  <link name="gps_link" />
  
  <joint name="output_link_joint" type="fixed">
    <parent link="base_link" />
    <child link="output_link" />
    <origin rpy="0 0 0" xyz="-0.825123 0 -0.942" />
  </joint>

  <joint name="imu_link_joint" type="fixed">
    <parent link="base_link" />
    <child link="compensate_imu_link" />
    <origin rpy="0 0 0" xyz="0 0 0" />
  </joint>

  <joint name="laser_link_joint" type="fixed">
    <parent link="base_link" />
    <child link="rslidar" />
    <origin rpy="0 0 0" xyz="-0.000123 0 0.1374" />
  </joint>

  <joint name="gps_link_joint" type="fixed">
    <parent link="base_link" />
    <child link="gps_link" />
    <origin rpy="0 0 0" xyz="-0.878521 -0.319632 0.0661" />
  </joint>

</robot>
